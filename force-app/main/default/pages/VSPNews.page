<apex:page controller="VSPNews">
    <apex:form>
        <apex:image value="/img/icon/livechattranscript16.png" />
        <apex:pageBlock
            title="Olá {!account.name}!, você tem um perfil {!account.Perfil__c}"
        >
            As mais recentes notícias do portal do Sicredi!
        </apex:pageBlock>
        <apex:pageBlock title="Noticias quentes">
            <apex:pageblockTable value="{!leadList}" var="f">
                <apex:column value="{!f}" />
            </apex:pageblockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>

